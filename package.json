{
	"type": "module",
	"name": "@intrnl/bluesky-client",
	"description": "Lightweight API client for Bluesky/AT Protocol",
	"version": "0.1.1",
	"author": "intrnl",
	"license": "MIT",
	"repository": {
		"url": "https://codeberg.org/intrnl/bluesky-client"
	},
	"packageManager": "pnpm@8.6.5",
	"files": [
		"dist",
		"!dist/tsconfig.tsbuildinfo"
	],
	"exports": {
		"./agent": {
			"types": "./dist/agent.d.ts",
			"import": "./dist/agent.js"
		},
		"./atp-schema": {
			"types": "./dist/atp-schema.d.ts"
		},
		"./events": {
			"types": "./dist/events.d.ts",
			"import": "./dist/events.js"
		},
		"./jwt": {
			"types": "./dist/jwt.d.ts",
			"import": "./dist/jwt.js"
		},
		"./xrpc-utils": {
			"types": "./dist/xrpc-utils.d.ts",
			"import": "./dist/xrpc-utils.js"
		},
		"./xrpc": {
			"types": "./dist/xrpc.d.ts",
			"import": "./dist/xrpc.js"
		}
	},
	"scripts": {
		"build:lib": "tsc",
		"build:schema": "node ./scripts/generate-atp-schema.js",
		"prepublishOnly": "pnpm run build:schema && pnpm run build:lib",
		"fmt": "prettier --cache --write ."
	},
	"devDependencies": {
		"@tiptap/core": "^2.0.3",
		"fast-glob": "^3.3.0",
		"graphemer": "^1.4.0",
		"prettier": "^2.8.8",
		"typescript": "^5.1.6"
	}
}
